# AI SME Evaluation Platform Configuration

# Application Settings
PORT=9777
ENVIRONMENT=development

# Model Configuration
MODEL_PROVIDER=gpt  # Options: gpt, claude
GPT_MODEL=gpt-4o-mini
CLAUDE_MODEL=claude-3-sonnet-20240229

# LiteLLM Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
LITELLM_PROXY_URL=  # Optional: URL to LiteLLM proxy endpoint

# AutoGen Configuration
AUTOGEN_TEMPERATURE=0.1
AUTOGEN_MAX_TOKENS=2000

# Evaluation Settings
MAX_CONCURRENT_EVALUATIONS=5
EVALUATION_TIMEOUT=300  # seconds

# Agent Communication Settings
AGENT_COMMUNICATION_TIMEOUT=60  # seconds per agent
LLM_REQUEST_TIMEOUT=45  # seconds per LLM call
AGENT_RETRY_ATTEMPTS=2
AGENT_RETRY_DELAY=1.0  # seconds
